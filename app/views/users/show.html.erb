<h1> <%=@user.name%> </h1>
<table>
  <tr><th>joined:</th><td> <%=@user.created_at.strftime('%m/%d/%Y')%> </td></tr>
 <tr><th>uid:</th><td> <%=@user.id %></td></tr>
</table>
<h4>furbabies</h4>
<table>
  <%@tokens.each do |t|%>
   <tr><td><%=t.created_at%></td>
    <td><%=t.vibes%></td></tr>
  <%end%>
</table>
<% if @user.id == current_user.id  %>
 <%= form_with model: Token do |f|%>
   <%= f.submit "Mint Token" %>
 <%end%>
<h5><i>This is you.</i></h5>
<% end %>
